@page
@model teste.Pages.MomentoCRUD.DetailsModel

@{
    ViewData["Title"] = "Descrição Completa";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4><b>Código:</b> @Model.Momento.IdMomento.ToString("D4")</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Momento.Descricao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Momento.Descricao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Momento.Data)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Momento.Data)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Momento.IdMomento">Alterar</a> |
    <a asp-page="./Index">Voltar</a>
</div>
